{
    "name": "mortezaa97/regions",
    "description": "A Laravel package for managing provinces and counties",
    "keywords": [
        "mortezaa97",
        "regions"
    ],
    "homepage": "https://github.com/mortezaa97/regions",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Morteza jafari",
            "email": "mortezajafari76@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Mortezaa97\\Regions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mortezaa97\\Regions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mortezaa97\\Regions\\RegionsServiceProvider"
            ],
            "aliases": {
                "Regions": "Mortezaa97\\Regions\\RegionsFacade"
            }
        }
    }
}
